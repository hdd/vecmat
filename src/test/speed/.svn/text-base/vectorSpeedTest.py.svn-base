from vecmat.vector import Vector


def sumVec():
	v1=Vector([3,2,1])
	v2=Vector([1,2,3])
	v1+v2
	
def subVec():
	v1=Vector([3,2,1])
	v2=Vector([1,2,3])
	v1-v2

def mulVecFloat():
	v1=Vector([3,2,1])
	f1=10.0
	v1*f1
	
def mulVecInt():
	v1=Vector([3,2,1])
	f1=10
	v1*f1
	
def divVecFloat():
	v1=Vector([3,2,1])
	f1=10.0
	v1/f1
	
def divVecInt():
	v1=Vector([3,2,1])
	f1=10
	v1/f1

def dotVec():
	v1=Vector([3,2,1])
	Vector([0,1,2]).dot(v1)
	
def crossVec():
	v1=Vector([3,2,1])
	Vector([0,1,2]).cross(v1)	
	
def normVec():
	Vector([3,2,1]).normalize()
	
def angleBetweenVec():
	v1=Vector([3,2,1])
	Vector([0,2,3]).angleBetween(v1)
	
